{
    "name": "songbook",
    "homepage": "https://zapralex.github.io/react-gh-pages",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@babel/core": "7.9.0",
        "@babel/plugin-transform-react-jsx": "^7.21.0",
        "@sberid/js-sdk": "^3.0.2",
        "@svgr/webpack": "4.3.3",
        "@testing-library/jest-dom": "^5.11.9",
        "@testing-library/react": "^11.2.5",
        "@testing-library/user-event": "^12.8.1",
        "@types/enzyme-adapter-react-16": "^1.0.6",
        "@types/react-transition-group": "^4.4.4",
        "axios": "^0.25.0",
        "babel-jest": "^24.9.0",
        "babel-loader": "8.1.0",
        "babel-plugin-named-asset-import": "^0.3.6",
        "babel-preset-react-app": "^9.1.2",
        "camelcase": "^5.3.1",
        "case-sensitive-paths-webpack-plugin": "2.3.0",
        "classnames": "^2.2.6",
        "css-loader": "3.4.2",
        "css-vars-ponyfill": "^2.1.2",
        "dotenv": "8.2.0",
        "dotenv-expand": "5.1.0",
        "enzyme-to-json": "^3.6.1",
        "file-loader": "4.3.0",
        "fs-extra": "^8.1.0",
        "gh-pages": "^5.0.0",
        "html-webpack-plugin": "4.0.0-beta.11",
        "http-proxy-middleware": "^1.0.6",
        "identity-obj-proxy": "3.0.0",
        "jest": "^29.5.0",
        "jest-resolve": "24.9.0",
        "jest-watch-typeahead": "0.4.2",
        "lodash": "^4.17.21",
        "mini-css-extract-plugin": "0.9.0",
        "node-sass": "^6.0.0",
        "optimize-css-assets-webpack-plugin": "5.0.3",
        "pnp-webpack-plugin": "1.6.4",
        "postcss-flexbugs-fixes": "4.1.0",
        "postcss-loader": "3.0.0",
        "postcss-normalize": "8.0.1",
        "postcss-preset-env": "6.7.0",
        "postcss-safe-parser": "4.0.1",
        "prop-types": "^15.7.2",
        "react": "^16.13.1",
        "react-app-polyfill": "^1.0.6",
        "react-autosuggest": "^10.1.0",
        "react-card-flip": "^1.0.11",
        "react-dev-utils": "^11.0.4",
        "react-dom": "^16.13.1",
        "react-hook-form": "^7.6.2",
        "react-idle-timer": "^4.3.6",
        "react-modal": "^3.11.2",
        "react-number-format": "^4.7.3",
        "react-player": "^2.9.0",
        "react-query": "^3.34.7",
        "react-router-dom": "^5.2.0",
        "react-textarea-autosize": "^8.2.0",
        "react-transition-group": "^4.4.2",
        "resolve": "1.15.0",
        "resolve-url-loader": "^3.1.2",
        "sass": "^1.32.13",
        "sass-loader": "^10.1.1",
        "semver": "6.3.0",
        "style-loader": "0.23.1",
        "swiper": "^6.6.1",
        "terser-webpack-plugin": "2.3.5",
        "ts-pnp": "1.1.6",
        "url-loader": "2.3.0",
        "use-debounce": "^6.0.1",
        "webpack": "4.42.0",
        "webpack-dev-server": "^3.11.0",
        "webpack-manifest-plugin": "2.2.0",
        "workbox-webpack-plugin": "4.3.1"
    },
    "scripts": {
        "start": "node scripts/start.js",
        "start:https": "HTTPS=true node scripts/start.js",
        "build": "node scripts/build.js",
        "test": "node scripts/test.js",
        "test:coverage": "yarn test --coverage --watchAll=false",
        "lint": "eslint --ext .js,.jsx,.ts,.tsx src/",
        "format": "eslint . --ext .js,.jsx,.ts,.tsx --fix && prettier --write '**/*{.css,.scss,.html,.json}'",
        "docker-build": "docker build --no-cache -t songbook-web .",
        "docker-run": "docker run -d --name songbook-web -p 80:80 songbook-web",
        "docker-stop": "docker stop songbook-web",
        "docker-start": "docker start songbook-web",
        "docker-rm": "docker rm songbook-web",
        "push-image": "mvn -Dmaven.test.skip=true clean package docker:build docker:push -Pdev -Ddocker.buildArg.profile=dev",
        "predeploy": "yarn run build",
        "deploy": "gh-pages -d build"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not op_mini all"
    ],
    "devDependencies": {
        "@babel/plugin-proposal-decorators": "^7.10.5",
        "@hookform/devtools": "^3.1.0",
        "@testing-library/react-hooks": "^5.1.0",
        "@types/classnames": "^2.2.11",
        "@types/enzyme": "^3.10.8",
        "@types/jest": "^26.0.15",
        "@types/lodash": "^4.14.165",
        "@types/node": "^14.14.6",
        "@types/qrcode.react": "^1.0.1",
        "@types/react": "^16.9.56",
        "@types/react-autosuggest": "^10.1.4",
        "@types/react-dom": "^16.9.9",
        "@types/react-modal": "^3.10.6",
        "@types/react-router-dom": "^5.1.6",
        "@typescript-eslint/eslint-plugin": "^4.2.0",
        "@typescript-eslint/parser": "^4.2.0",
        "babel-eslint": "10.1.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.6",
        "eslint": "^6.6.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-config-react-app": "^5.2.1",
        "eslint-loader": "3.0.3",
        "eslint-plugin-flowtype": "4.6.0",
        "eslint-plugin-import": "2.20.1",
        "eslint-plugin-jest": "^23.20.0",
        "eslint-plugin-jest-dom": "^3.6.5",
        "eslint-plugin-jsx-a11y": "6.2.3",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "7.19.0",
        "eslint-plugin-react-hooks": "^1.6.1",
        "eslint-plugin-testing-library": "^3.10.2",
        "husky": "^4.3.0",
        "jest-environment-jsdom-fifteen": "^1.0.2",
        "jest-sonar-reporter": "^2.0.0",
        "lint-staged": "^10.5.0",
        "nock": "^13.2.4",
        "prettier": "2.5.1",
        "react-test-renderer": "^16.13.1",
        "typescript": "4.2.4"
    },
    "jest": {
        "testResultsProcessor": "jest-sonar-reporter",
        "collectCoverage": true,
        "collectCoverageFrom": [
            "src/**/*.{ts,tsx}",
            "!src/serviceWorker.js",
            "!src/setupProxy.js"
        ],
        "roots": [
            "<rootDir>/src"
        ],
        "modulePathIgnorePatterns": [
            "<rootDir>/src/static"
        ],
        "setupFiles": [
            "react-app-polyfill/jsdom"
        ],
        "setupFilesAfterEnv": [
            "<rootDir>/jest-setup.ts"
        ],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
            "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
        ],
        "testEnvironment": "jest-environment-jsdom-fifteen",
        "transform": {
            "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
            "^.+\\.module\\.(css|sass|scss)$"
        ],
        "modulePaths": [],
        "moduleNameMapper": {
            "^react-native$": "react-native-web",
            "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
        },
        "moduleFileExtensions": [
            "web.js",
            "js",
            "web.ts",
            "ts",
            "web.tsx",
            "tsx",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ],
        "watchPlugins": [
            "jest-watch-typeahead/filename",
            "jest-watch-typeahead/testname"
        ]
    },
    "babel": {
        "plugins": [
            [
                "@babel/plugin-proposal-decorators",
                {
                    "legacy": true
                }
            ],
            "@babel/plugin-transform-react-jsx"
        ],
        "presets": [
            "react-app"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*{.css,.scss,.html,.json}": [
            "prettier --write"
        ],
        "*{.js,.jsx, .ts, .tsx}": [
            "eslint --fix"
        ]
    }
}
